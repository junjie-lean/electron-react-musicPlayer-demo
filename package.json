{
  "name": "electron-react-hook",
  "projectName": "electron-react-hook",
  "version": "1.2.2",
  "main": "main.js",
  "port": 5000,
  "scripts": {
    "dev": "npx cross-env NODE_ENV=development MEASURE=false node ./scripts/dev-start.js",
    "build": "cross-env NODE_ENV=production node ./scripts/dist.js",
    "build:dist": "electron-builder",
    "build:nodist": "npx cross-env NODE_ENV=production MEASURE=false npx webpack  --config ./config/webpack.base.config",
    "release": "cross-env GH_TOKEN=a246132f3bb9507bff9a3f9adb2aa815bd0e3901 electron-builder",
    "prebuild": "npx cross-env NODE_ENV=production MEASURE=false node ./scripts/build.js",
    "prerelease": "npm run build:nodist"
  },
  "postinstall": "electron-builder install-app-deps",
  "build": {
    "appId": "com.localMusilPlayer.electron-app",
    "productName": "本地音乐播放器",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "statisc/*",
      "main.js",
      "main.min.js",
      "main-store.js",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "mac": {
      "target": "dmg",
      "category": "public.app-category.productivity",
      "icon": "statisc/timg.icns"
    },
    "dmg": {
      "background": "statisc/bg.png",
      "icon": "statisc/timg.icns",
      "title": "installer",
      "iconSize": 100,
      "contents": [
        {
          "x": 380,
          "y": 280,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 110,
          "y": 280,
          "type": "file"
        }
      ],
      "window": {
        "width": 500,
        "height": 500
      }
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "public.app-category.productivity",
      "icon": "statisc/timg.icns"
    },
    "win": {
      "target": [
        {
          "target": "nsis-web",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "statisc/icon.ico",
      "artifactName": "${productName}-Web-Setup-${version}.${ext}"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": false,
      "installerIcon":"./statisc/timg.ico",
      "createDesktopShortcut":true,
      "createStartMenuShortcut":true
    }
  },
  "author": "junjie.lean <junjie.lean@foxmail.com>",
  "license": "MIT",
  "description": "electron webpack react hook antd",
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "antd": "4.3.4",
    "axios": "0.19.2",
    "electron-is-dev": "^1.2.0",
    "electron-store": "^5.2.0",
    "elemon": "^5.0.3",
    "react-perfect-scrollbar": "^1.5.8",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-decorators": "7.10.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.10.1",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/polyfill": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@babel/register": "7.10.1",
    "@babel/runtime": "7.10.2",
    "@babel/runtime-corejs3": "7.10.2",
    "@svgr/webpack": "5.4.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-autobind-class-methods": "5.0.1",
    "babel-plugin-import": "1.13.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-runtime": "6.26.0",
    "chai": "4.2.0",
    "chalk": "4.1.0",
    "clean-webpack-plugin": "3.0.0",
    "core-js": "3.6.5",
    "cross-env": "7.0.2",
    "cross-spawn": "7.0.3",
    "css-loader": "3.6.0",
    "cssnano": "4.1.10",
    "electron": "9.0.5",
    "electron-builder": "^22.7.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "file-loader": "6.0.0",
    "happypack": "5.0.1",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "jest": "26.0.1",
    "jsdom": "16.2.2",
    "less": "3.11.3",
    "less-loader": "6.1.1",
    "lodash": "4.17.15",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "nodemon": "1.19.4",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "progress-bar-webpack-plugin": "2.1.0",
    "raw-loader": "4.0.1",
    "react": "16.13.1",
    "react-dev-utils": "10.2.1",
    "react-dom": "16.13.1",
    "react-loading": "2.0.3",
    "react-router-dom": "5.2.0",
    "react-router-transition": "2.0.0",
    "react-test-renderer": "16.13.1",
    "sass-loader": "8.0.2",
    "speed-measure-webpack-plugin": "1.3.3",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "3.0.5",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-parallel-uglify-plugin": "1.1.2",
    "zip-local": "0.3.4"
  }
}
